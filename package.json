{
  "name": "cozy-konnector-malakoffmederic",
  "version": "1.0.0",
  "description": "Import your Malakoff Mederic bills",
  "repository": {
    "type": "git",
    "url":
      "git+https://github.com/konnectors/cozy-konnector-malakoffmederic.git"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0",
  "main": "src/index.js",
  "renovate": {
    "extends": ["config:base"],
    "patch": {
      "automerge": true
    }
  },
  "scripts": {
    "standalone": "DEBUG=* cozy-konnector-standalone src/index.js",
    "standalone:record": "REPLAY=record cozy-konnector-standalone src/index.js",
    "standalone:replay": "REPLAY=replay cozy-konnector-standalone src/index.js",
    "dev": "cozy-konnector-dev src/index.js",
    "build": "webpack",
    "format": "prettier --write '**/*.{js,json}'",
    "deploy":
      "git-directory-deploy --directory build/ --branch build --repo=https://github.com/konnectors/cozy-konnector-malakoffmederic.git",
    "deploy:prod":
      "git-directory-deploy --directory build/ --branch latest --repo=https://github.com/konnectors/cozy-konnector-malakoffmederic.git"
  },
  "dependencies": {
    "cozy-konnector-libs": "3.3.0",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "4.4.1",
    "git-directory-deploy": "1.5.1",
    "prettier": "1.10.2",
    "webpack": "3.11.0"
  }
}
